<scene>
	<timeline type="offset" name="global timeline" offset="0">
		<children>
			<timeline type="offset" name="5secoffset" offset="-5"/>
			<timeline type="offset" name="3secoffset" offset="-3">
				<children>
					<timeline name="test it!" duration="10" pre="repeat" post="repeat">
						<keyframes>
							<keyframe type="stop" name="asd" event_time="4"/>
							<keyframe type="null" name="hue" event_time="5"/>
							<keyframe type="loop" name="xd" event_time="9" event_action="restart" target_time="0" loop_count="4"/>
						</keyframes>
					</timeline>
				</children>
			</timeline>
		</children>
	</timeline>
	<node name="rectNode" timeline="global timeline">
		<children>
			<node name="textNode" timeline="global timeline">
				<plugins>
					<plugin type="transform" timeline="global timeline">
						<transform type="fwd_center">
							<keyV3 time="0" x="0" y="0" z="0"/>
						</transform>
						<transform type="translation">
							<keyV3 time="0" x="0" y="0" z="0"/>
						</transform>
						<transform type="rotation">
							<keyF time="0" value="0"/>
						</transform>
						<transform type="scale">
							<keyV3 time="0" x="1" y="1" z="1"/>
						</transform>
						<transform type="inv_center">
							<keyV3 time="0" x="0" y="0" z="0"/>
						</transform>
					</plugin>
					<plugin type="color">
						<paramV4 timeline="global timeline">
							<keyV4 time="0" x="1" y="0" z="0" w="1"/>
						</paramV4>
					</plugin>
					<plugin type="text" text="asd" font="../dep/Media/fonts/arial.ttf">
						<paramF key="font_size" timeline="global timeline">
							<keyF time="0" value="50"/>
						</paramF>
						<paramF key="blur_size" timeline="global timeline">
							<keyF time="0" value="0"/>
						</paramF>
					</plugin>
				</plugins>
			</node>
		</children>
		<plugins>
			<plugin type="transform" timeline="3secoffset">
				<transform type="fwd_center">
					<keyV3 time="0" x="0" y="0" z="0"/>
				</transform>
				<transform type="translation">
					<keyV3 time="0" x="0" y="0" z="0"/>
				</transform>
				<transform type="rotation">
					<keyF time="0" value="0"/>
					<keyF time="2" value="360"/>
				</transform>
				<transform type="scale">
					<keyV3 time="0" x="1" y="1" z="1"/>
				</transform>
				<transform type="inv_center">
					<keyV3 time="0" x="0" y="0" z="0"/>
				</transform>
			</plugin>
			<plugin type="rect">
				<paramF key="width" timeline="5secoffset">
					<keyF time="0" value="2"/>
					<keyF time="7" value="1"/>
				</paramF>
				<paramF key="height" timeline="5secoffset">
					<keyF time="0" value="1"/>
					<keyF time="7" value="2"/>
				</paramF>
			</plugin>
			<plugin type="color">
				<paramV4 timeline="global timeline">
					<keyV4 time="0" x="0.5" y="0" z="0" w="0.300000012"/>
					<keyV4 time="5" x="0" y="0" z="0.5" w="0.300000012"/>
				</paramV4>
			</plugin>
		</plugins>
	</node>
</scene>

